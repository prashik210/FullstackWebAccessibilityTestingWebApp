{
    "manifest_version": 3,
      "name": "Capgemini React Web Accessibility Testing",
      "version": "1.0",
      "description": "Get Accessibility test results from a webpage",
      "author": "Prashik Ramteke",
    "short_name": "Capgemini Accessibility",
    "icons": {
        "16": "images/favicon-16x16.png",
        "32": "images/favicon-32x32.png",
        "48": "images/android-chrome-512x512.png",
        "128": "images/android-chrome-512x512.png"
      },
    "permissions": [
      "activeTab",
      "tabs",
      "scripting"
    ],
    "action": {
      "default_popup": "index.html",
      "default_title": "Capgemini React Web Accessibility Feature",
      "default_icon": {
        "16": "images/favicon-16x16.png",
        "32": "images/favicon-32x32.png",
        "48": "images/android-chrome-512x512.png",
        "128": "images/android-chrome-512x512.png"
      }
    }
  }